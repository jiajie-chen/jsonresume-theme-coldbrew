{
    "author": "Jiajie Chen",
    "name": "@jiajie-chen/jsonresume-theme-coldbrew",
    "description": "Caffeine theme for JSON Resume, modified for my personal use.",
    "version": "0.1.0",
    "type": "module",
    "keywords": [
        "json",
        "resume",
        "jsonresume",
        "json-resume"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/jiajie-chen/jsonresume-theme-coldbrew"
    },
    "license": "MIT",
    "module": "./dist/main.js",
    "exports": {
        ".": {
            "import": "./dist/main.js"
        }
    },
    "scripts": {
        "prepare:husky": "husky install",
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint:es": "eslint --ext .js,jsx,ts,tsx,cjs --ignore-path .gitignore .",
        "lint:style": "stylelint src/**/*.{css,scss}",
        "lint:all": "npm run lint:es || npm run lint:style",
        "format:all": "prettier --ignore-unknown --write .",
        "type-gen": "node ./scripts/type-gen.cjs"
    },
    "devDependencies": {
        "@preact/preset-vite": "^2.5.0",
        "@types/luxon": "^3.2.0",
        "@typescript-eslint/eslint-plugin": "^5.54.0",
        "@typescript-eslint/parser": "^5.54.0",
        "eslint": "^8.35.0",
        "eslint-config-preact": "^1.3.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-jsdoc": "^40.0.1",
        "husky": "^8.0.3",
        "json-schema-to-typescript": "^12.0.0",
        "lint-staged": "^13.1.2",
        "prettier": "^2.8.4",
        "resume-schema": "^1.0.0",
        "resumed": "^3.0.1",
        "sass": "^1.58.3",
        "stylelint": "^15.2.0",
        "stylelint-config-standard": "^30.0.1",
        "stylelint-config-standard-scss": "^7.0.1",
        "typescript": "^4.9.5",
        "vite": "^4.1.4"
    },
    "dependencies": {
        "@shopify/address": "^4.0.6",
        "luxon": "^3.2.1",
        "preact": "^10.13.0",
        "preact-render-to-string": "^5.2.6"
    },
    "stylelint": {
        "extends": [
            "stylelint-config-standard",
            "stylelint-config-standard-scss"
        ]
    },
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,cjs}": "eslint",
        "*.css": "stylelint",
        "*.scss": "stylelint --syntax=scss",
        "*": "prettier --ignore-unknown --write"
    }
}
